<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>controllers.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controllers.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controllers.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchMobileController</servlet-name>
        <servlet-class>controllers.SearchMobileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadMobileController</servlet-name>
        <servlet-class>controllers.LoadMobileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMobileController</servlet-name>
        <servlet-class>controllers.DeleteMobileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMobileController</servlet-name>
        <servlet-class>controllers.UpdateMobileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMobileController</servlet-name>
        <servlet-class>controllers.AddMobileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchPriceController</servlet-name>
        <servlet-class>controllers.SearchPriceController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCartController</servlet-name>
        <servlet-class>controllers.AddToCartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewCartController</servlet-name>
        <servlet-class>controllers.ViewCartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCartController</servlet-name>
        <servlet-class>controllers.UpdateCartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveFromCartController</servlet-name>
        <servlet-class>controllers.RemoveFromCartController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadMobileUserController</servlet-name>
        <servlet-class>controllers.LoadMobileUserController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/MainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchMobileController</servlet-name>
        <url-pattern>/SearchMobileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadMobileController</servlet-name>
        <url-pattern>/LoadMobileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMobileController</servlet-name>
        <url-pattern>/DeleteMobileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMobileController</servlet-name>
        <url-pattern>/UpdateMobileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMobileController</servlet-name>
        <url-pattern>/AddMobileController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchPriceController</servlet-name>
        <url-pattern>/SearchPriceController</url-pattern>
    </servlet-mapping>
